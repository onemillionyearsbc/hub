    "css-loader": "^2.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "font-awesome-loader": "^1.0.2",
    "font-awesome-webpack": "0.0.5-beta.2",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1",
