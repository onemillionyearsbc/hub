    "composer-admin": "^0.20.1",
    "composer-cli": "^0.20.1",
    "composer-client": "^0.20.1",
    "composer-common": "^0.20.1",
    "composer-connector-embedded": "^0.20.1",
    "composer-cucumber-steps": "^0.20.1",
    "chai": "latest",
    "chai-as-promised": "latest",
    "cucumber": "^2.2.0",
    "eslint": "latest",
    "nyc": "latest",
    "mkdirp": "latest",
    "mocha": "latest"
  }
